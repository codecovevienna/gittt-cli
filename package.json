{
  "name": "gittt-cli",
  "version": "1.0.0",
  "description": "Tracking time with CLI into a git repository",
  "main": "index.ts",
  "scripts": {
    "start": "./node_modules/.bin/ts-node index.ts",
    "test": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true ./node_modules/.bin/mocha test ./test/**/*.test.ts",
    "test:ci": "./node_modules/.bin/nyc npm run test",
    "lint": "./node_modules/.bin/tslint --project ./tsconfig.json",
    "lint:fix": "./node_modules/.bin/tslint --fix --project ./tsconfig.json",
    "codecov": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:codecovevienna/gittt-cli.git"
  },
  "keywords": [
    "git",
    "time",
    "tracking"
  ],
  "author": "CodeCove Vienna",
  "contributors": [
    {
      "name": "eiabea",
      "email": "developer@eiabea.com",
      "url": "https://github.com/eiabea"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.7",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^5.1.0",
    "@types/inquirer": "6.0.1",
    "@types/lodash": "^4.14.126",
    "@types/shelljs": "^0.8.5",
    "@types/uuid": "^3.4.4",
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "shelljs": "^0.8.3",
    "simple-git": "^1.113.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.11",
    "codecov": "^3.4.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2",
    "tslint": "^5.16.0"
  }
}